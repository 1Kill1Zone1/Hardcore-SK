   <RecipeDef ParentName="RecipeAdvancedProsthesis">
    <defName>CreateExoskeletonSuit</defName>
    <label>Craft exoskeleton suit</label>
    <description>Crafts an exoskeleton suit from steel and plasteel. Requires the 3D printer and a moderate amount of time.</description>
    <jobString>Crafting bionic.</jobString>
	<ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>SyntheticMaterials</li>
               </thingDefs>
            </filter>
            <count>16</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Spare_Parts</li>
               </thingDefs>
            </filter>
            <count>14</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>ArtificialBone</li>
               </thingDefs>
            </filter>
            <count>12</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Parts_vancidium</li>
               </thingDefs>
            </filter>
            <count>10</count>
         </li>
    </ingredients>
	<fixedIngredientFilter>
			<thingDefs>
            <li>SyntheticMaterials</li>
            <li>Spare_Parts</li>
            <li>ArtificialBone</li>
            <li>Parts_vancidium</li>
			</thingDefs>
		</fixedIngredientFilter>	
	<products>
		<ExoskeletonSuit>1</ExoskeletonSuit>
	</products>
  </RecipeDef>
   
