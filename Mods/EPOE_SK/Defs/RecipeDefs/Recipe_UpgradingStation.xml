   <RecipeDef ParentName="RecipeAdvancedProsthesis">
      <defName>CreateAdvancedBionicEye</defName>
      <label>Upgrade bionic eye</label>
      <description>Upgrades a bionic eye with vancidium.</description>
      <jobString>Upgrading bionic.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Hyperweave</li>
               </thingDefs>
            </filter>
            <count>15</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Parts_vancidium</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BionicEye</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Hyperweave</li>
            <li>Parts_vancidium</li>
            <li>BionicEye</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <AdvancedBionicEye>1</AdvancedBionicEye>
      </products>
   </RecipeDef>
   
