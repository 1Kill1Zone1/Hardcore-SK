   <RecipeDef ParentName="RecipeAdvancedProsthesis">
      <defName>CreateGTBionicEye</defName>
      <label>Create cybernetic bionic eye</label>
      <description>The advanced version of the bionic eye. The second generation of bionic upgrades, typically reserved for top brass military forces.</description>
      <jobString>Creating cybernetic bionic eye.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Biomatter</li>
               </thingDefs>
            </filter>
            <count>25</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BioMicrochips</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>AdvancedBionicEye</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Biomatter</li>
            <li>BioMicrochips</li>
            <li>AdvancedBionicEye</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <GTEye>1</GTEye>
      </products>
   </RecipeDef>
   
  
