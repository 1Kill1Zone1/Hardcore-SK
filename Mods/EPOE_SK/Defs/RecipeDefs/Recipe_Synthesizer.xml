   <RecipeDef ParentName="RecipeSyntheticOrgan">
      <defName>CreateAIChip</defName>
      <label>Craft AI chip</label>
      <description>Crafts an AI chip. Restores brain functions, and very hard to make. Needs gold for highest quality circuitry.</description>
      <jobString>Creating synthetic organ.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Parts_CarbonAlloy</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BioMicrochips</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Parts_CarbonAlloy</li>
            <li>BioMicrochips</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <AIChip>1</AIChip>
      </products>
   </RecipeDef>
   
