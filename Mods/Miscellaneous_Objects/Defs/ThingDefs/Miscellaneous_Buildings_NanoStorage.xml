<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

<ThingDef ParentName="BuildingBase">
		<defName>NanoStorage</defName>
		<label>Nano Storage</label>
		<description>This building provides nano storage capabilities.</description>
		<thingClass>NanoStorage.Building_NanoStorage</thingClass>
		<category>Building</category>
		<graphicData>
			<texPath>Things/Building/NanoStorage/NanoStorage</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<statBases>
			<WorkToMake>9750</WorkToMake>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Beauty>2</Beauty>
		</statBases>
		<castEdgeShadows>false</castEdgeShadows>
		<size>(1,1)</size>
			<stuffCategories>
			<li>Metallic</li>
			<li>Plastic</li>
			</stuffCategories>
			<costStuffCount>20</costStuffCount>
		<costList>
			<Component>5</Component>
			<Microchips>1</Microchips>
		</costList>
		<killedLeavings>
			<ChunkSlagSteel>1</ChunkSlagSteel>
		</killedLeavings>
		<rotatable>true</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<DesignationCategory>Misc</DesignationCategory>
		<surfaceType>Item</surfaceType>
		<tickerType>Normal</tickerType>
		<building>
			<preventDeterioration>true</preventDeterioration>
			<fixedStorageSettings>
				<priority>Important</priority>
				<filter>
					<categories>
						<li>Resources</li>
						<li>Items</li>
						<li>Weapons</li>
						<li>Apparel</li>
						<li>BodyParts</li>
					</categories>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Important</priority>
				<filter>
					<specialFiltersToDisallow>
						<li>AllowRotten</li>
					</specialFiltersToDisallow>
				</filter>
			</defaultStorageSettings>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<researchPrerequisites><li>SK_NanotechnologyI</li></researchPrerequisites>
	</ThingDef>

</ThingDefs>
