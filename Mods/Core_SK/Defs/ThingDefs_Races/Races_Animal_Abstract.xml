<Defs>
	
	<!-- JUNK DEF -->
	
	<ThingDef Abstract="True" ParentName="AnimalThingBase" Name="BaseAbstract">
		<statBases>
			<Mass>1.5</Mass>
			<MoveSpeed>5.5</MoveSpeed>
			<ComfyTemperatureMin>-30</ComfyTemperatureMin>
			<MarketValue>90</MarketValue>
			<ArmorPenetration>0.01</ArmorPenetration>
		</statBases>
		<verbs>
			<li>
				<verbClass>Verb_MeleeAttack</verbClass>
				<defaultCooldownTime>0.2</defaultCooldownTime>
				<meleeDamageBaseAmount>200</meleeDamageBaseAmount>
				<meleeDamageDef>Bite</meleeDamageDef>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
			</li>
		</verbs>
    <race>
		<body>QuadrupedAnimalWithPawsAndTail</body>
		<baseBodySize>0.24</baseBodySize>
		<baseHungerRate>0.23</baseHungerRate>
		<baseHealthScale>0.4</baseHealthScale>
		<foodType>VegetarianRoughAnimal</foodType>
		<wildness>0.75</wildness>
		<mateMtbHours>12</mateMtbHours>
		<nuzzleMtbHours>120</nuzzleMtbHours>
		<gestationPeriodDays>8.5</gestationPeriodDays>
		<litterSizeCurve>
			<points>
				<li>(2.5, 0)</li>
				<li>(3, 1)</li>
				<li>(4, 1)</li>
				<li>(4.5, 0)</li>
			</points>
		</litterSizeCurve>
		<lifeExpectancy>8</lifeExpectancy>
		<lifeStageAges>
			<li>
				<def>AnimalBaby</def>
				<minAge>0</minAge>
			</li>
			<li>
				<def>AnimalJuvenile</def>
				<minAge>0.1</minAge>
			</li>
			<li>
				<def>AnimalAdult</def>
				<minAge>0.3</minAge>
				<soundWounded>Pawn_Hare_Wounded</soundWounded>
				<soundDeath>Pawn_Hare_Death</soundDeath>
				<soundCall>Pawn_Hare_Call</soundCall>
				<soundAngry>Pawn_Hare_Angry</soundAngry>
			</li>
		</lifeStageAges>
		<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
		<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
		<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>StandardAnimal</li>
		</tradeTags>
	</ThingDef>

  <PawnKindDef Abstract="True" ParentName="AnimalKindBase" Name="AbstractBase">
    <combatPower>15</combatPower>
    <wildSpawn_spawnWild>false</wildSpawn_spawnWild>
  </PawnKindDef>
  
