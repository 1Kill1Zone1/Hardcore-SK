  <RecipeDef>
    <defName>MakeSulfur</defName>
    <label>Obtain Sulfur</label>
    <description>Obtain Sulfur from Iron Ore. Sulfur occurs naturally as the pure element - native sulfur and as sulfide and sulfate minerals. It is useful for Gunpowder in crafting various types of turret ammo. Produces 10.</description>
    <jobString>Obtaining Sulfur.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1200</workAmount>
    <targetCountAdjustment>5</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>35</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
            <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Sulphates>10</Sulphates>
    </products>
    <workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>


