<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>BrewBeer</defName>
    <label>Make Beer</label>
    <description>Brew a batch of beer from hops.</description>
    <jobString>Brewing beer.</jobString>
    <workSpeedStat>BrewingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>600</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawHops</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
			<li>
				<filter>
					<categories>
						<li>Tableware</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawHops</li>
      </thingDefs>
			<categories>
				<li>Tableware</li>
			</categories>
    </fixedIngredientFilter>
    <products>
      <Beer>5</Beer>
    </products>
    <workSkill>Cooking</workSkill>
  </RecipeDef>


