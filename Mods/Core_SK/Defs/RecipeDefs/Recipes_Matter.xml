<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <!-- ============================================================== -->
  <!-- Convert to matter -->
  <!-- ============================================================== -->
  
    <!-- Raw resources -->
    <RecipeDef>
    <defName>ConvertResourcesRaw</defName>
    <label>Convert raw resources</label>
    <description>Convert raw resources into inorganic matter. Produces 5.</description>
    <jobString>Converting resources to matter.</jobString>
    <workAmount>800</workAmount>
		<workSpeedStat>ElectronicCraftingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>ResourcesRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>ResourcesRaw</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <Matter>5</Matter>
    </products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>16</minLevel>
			</li>
		</skillRequirements>
	<workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.5</workSkillLearnFactor>
  </RecipeDef>
  
 
