<RecipeDefs>

  <RecipeDef>
    <defName>OilRefining</defName>
    <label>Oil refining</label>
    <description>Refining crude oil.</description>
    <jobString>Oil producion.</jobString>
    <workAmount>1200</workAmount>
    <efficiencyStat>ButcheryMechanoidEfficiency</efficiencyStat>
	<effectWorking>Smith</effectWorking>
	<ingredients>
     <li>
        <filter>
          <thingDefs>
	<li>CrudeOil</li>
          </thingDefs>
        </filter>
        <count>35</count>
      </li>
    </ingredients>
    <products>
      <Polymers>20</Polymers>
      <Fuel>21</Fuel>
      <Sand>10</Sand>
      <Sulphates>2</Sulphates>
      <Paraffins>1</Paraffins>
      <SyntheticAmmonia>1</SyntheticAmmonia>
    </products>
    <fixedIngredientFilter>
          <thingDefs>
	<li>CrudeOil</li>
          </thingDefs>
    </fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
  </RecipeDef>


