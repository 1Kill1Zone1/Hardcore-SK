<RecipeDefs>
  
  <!-- ==================== 6x18mm Charged ========================== -->
  
  <RecipeDef ParentName="ChargeAmmoRecipeBase">
    <defName>MakeAmmo_6x18mmCharged</defName>
    <label>make 6x18mm Charged cartridge x80</label>
    <description>Craft 80 6x18mm Charged cartridges.</description>
    <jobString>Making 6x18mm Charged cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>SLDBar</li>
          </categories>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>USLDBar</li>
          </categories>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>MagneticMaterial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>SLDBar</li>
        <li>USLDBar</li>
      </categories>
      <thingDefs>
        <li>MagneticMaterial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_6x18mmCharged>80</Ammo_6x18mmCharged>
    </products>
  </RecipeDef>
  
