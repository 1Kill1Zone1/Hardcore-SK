<Resources>


  <!-- =========== RAW MEATS! SK MOD =========== 
  
  
  These defs properly configure meats from creatures. Vanilla auto-gen has a number of issues in HSK, and this file addresses that. This also makes everything much more detailed and accurate while also removing redundancy and bloat. ~Masq-->

  
  <ThingDef Name="ResourceBase" Abstract="True">
    <thingClass>ThingWithComps</thingClass>
    <label>unspecified resource</label>
	<description>This item lacks a proper description.</description>
    <category>Item</category>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <useHitPoints>true</useHitPoints>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <stackLimit>300</stackLimit>
    <comps>
      <li>
        <compClass>CompForbiddable</compClass>
      </li>
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <drawGUIOverlay>true</drawGUIOverlay>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
  </ThingDef>


	<ThingDef ParentName="ResourceBase">
		<defName>BeefMeat</defName>
		<label>Beef</label>
		<description>Beef is a category of meat that comes from bovine-like animals. Domestic Cows, Muffalo and Alphamuffalo produce Beef when butchered. Beef is a tender, lean meat with a decent fat content. It makes for good roasts and steaks.</description>
			<graphicData>
				<texPath>Things/Item/Meal/RoastedMeat/RoastedMeatE</texPath>
				<graphicClass>Graphic_Single</graphicClass>
			</graphicData>
		<tickerType>Rare</tickerType>
			<statBases>
				<MaxHitPoints>50</MaxHitPoints>
				<Flammability>1.0</Flammability>
				<DeteriorationRate>5</DeteriorationRate>
				<MarketValue>3.5</MarketValue>
			</statBases>
			<thingCategories>
				<li>MeatRawAll</li>
			</thingCategories>
		<stackLimit>300</stackLimit>
		<socialPropernessMatters>true</socialPropernessMatters>
			<ingestible>
				<preferability>DesperateOnly</preferability>
				<nutrition>0.45</nutrition>
				<eatEffect>EatMeat</eatEffect>
				<soundEat>RawMeat_Eat</soundEat>
				<ingestedDirectThought>AteRawMeat</ingestedDirectThought>
				<isMeat>true</isMeat>
			</ingestible>
			<comps>
				<li Class="CompProperties_Rottable">
				<compClass>CompRottable</compClass>
				<daysToRotStart>4</daysToRotStart>
				<rotDestroys>true</rotDestroys>
				</li>
			</comps>
	</ThingDef>


