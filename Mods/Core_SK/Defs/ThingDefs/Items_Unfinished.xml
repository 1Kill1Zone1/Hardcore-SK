<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingDef Name="UnfinishedBase" Abstract="True">
    <thingClass>UnfinishedThing</thingClass>
    <category>Item</category>
    <label>Unfinished thing</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <useHitPoints>true</useHitPoints>
    <isUnfinishedThing>true</isUnfinishedThing>
    <selectable>true</selectable>
    <tradeability>Never</tradeability>
    <drawerType>RealtimeOnly</drawerType>
    <!-- For CompForbiddable -->
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
    </statBases>
    <comps>
      <li>
        <compClass>CompForbiddable</compClass>
      </li>
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <thingCategories>
      <li>Unfinished</li>
    </thingCategories>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  <!--=================== Sculptures ======================-->

  <ThingDef ParentName="UnfinishedBase">
    <defName>UnfinishedSculpture</defName>
    <label>Unfinished Sculpture</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
      <li>Precious</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  
