<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

    
  <ThingDef ParentName="ApparelBase">
    <defName>Apparel_HuskerHood</defName>
    <label>Husker Hood</label>
    <description>A crude but rugged hood of the ferals.</description>
    <graphicData>
      <texPath>Things/Apparel/Feral/HuskerHood</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>50</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
		<CostList>
			<Components>5</Components>
		</CostList>
    <statBases>
		<ArmorRating_Blunt>0.09</ArmorRating_Blunt>
		<ArmorRating_Sharp>0.04</ArmorRating_Sharp>
		<Insulation_Heat>8</Insulation_Heat>
		<Insulation_Cold>-5</Insulation_Cold>
		<Weight>0.2</Weight>
		<Bulk>3</Bulk>
		<WornWeight>0.2</WornWeight>
		<WornBulk>0</WornBulk>
    </statBases>
    <equippedStatOffsets>
	  <MoveSpeed>-0.01</MoveSpeed>
      <SocialImpact>-0.2</SocialImpact>
	  <TrainAnimalChance>0.11</TrainAnimalChance>
	  <TameAnimalChance>0.13</TameAnimalChance>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <worngraphicPath>Things/Apparel/Feral/HuskerHood</worngraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <commonality>1</commonality>
      <tags>
        <li>FeralOutfit</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>RGBA(0.9,0.54,0.15,1)</only>
        </li>
        <li>
          <weight>15</weight>
          <only>RGBA(0.9,0.6,0.18,1)</only>
        </li>
        <li>
          <weight>20</weight>
          <only>RGBA(0.9,0.42,0.23,1)</only>
        </li>
      </options>
    </colorGenerator>
		<thingCategories>
			<li>HeaddressCat</li>
		</thingCategories>
  </ThingDef>


