	<!-- ============ Slag_Vest_Improved ============ -->
	<ThingDef ParentName="ArmorMedievalBase">
		<defName>Slag_Vest_Improved</defName>
		<label>Feral Vest</label>
		<description>An improvement of the crude slag vest, this armour has been adapted to the frame of the user, and has had its looks upgraded with a hand of paint. Watch out for fire and electricity.</description>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>40</costStuffCount>
		<costList>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<graphicData>
			<texPath>Things/Apparel/Vest_Improved/Vest_Improved</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<WorkToMake>6000</WorkToMake>
			<MaxHitPoints>180</MaxHitPoints>
			<ArmorRating_Blunt>0.40</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.30</ArmorRating_Sharp>
			<ArmorRating_Heat>0.10</ArmorRating_Heat>
			<Insulation_Cold>-9</Insulation_Cold>
			<Insulation_Heat>2</Insulation_Heat>
			<Mass>7</Mass>
			<Bulk>13</Bulk>
			<WornBulk>7</WornBulk>
			<EquipDelay>5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<CarryBulk>20</CarryBulk>
			<MoveSpeed>-0.15</MoveSpeed>
			<SocialImpact>0.05</SocialImpact>
			<ShootingAccuracyPawn>-0.1</ShootingAccuracyPawn>
			<AimingDelayFactor>0.16</AimingDelayFactor>
			<MeleeHitChance>-0.1</MeleeHitChance>
			<MeleeWeapon_CooldownMultiplier>0.15</MeleeWeapon_CooldownMultiplier>
		</equippedStatOffsets>
		<generateCommonality>1</generateCommonality>
		<apparel>
			<wornGraphicPath>Things/Apparel/Vest_Improved/Vest_Improved</wornGraphicPath>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<bodyPartGroups>
				<li>Torso</li>
			</bodyPartGroups>
			<tags>
				<li>PredatorsUnique</li>
				<li>PredatorsLight</li>
				<li>PredatorsMedium</li>
			</tags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<min>(0.5,0.5,0.5,1)</min>
					<max>(1,1,1,1)</max>
				</li>
			</options>
		</colorGenerator>
		<thingCategories>
			<li>VestsCat</li>
		</thingCategories>
		<modExtensions>
			<li Class="CombatExtended.VisibleAccessoryDefExtension">
				<order>1</order>
			</li>
		</modExtensions>
	</ThingDef>

	<!-- ============ Slag_Plate_Crude ============ -->

	<ThingDef ParentName="ArmorMedievalBase">
		<defName>Slag_Plate_Crude</defName>
		<label>Feral Plate Armour</label>
		<description>Constructed of recycled slag, this plate armour is encumbering but effective against bludgeoning. The gaps in the plates render it almost useless against animals, bullets, or fire. It's made of metal, so don't get electrocuted while wearing this.</description>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>55</costStuffCount>
		<costList>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<graphicData>
			<texPath>Things/Apparel/Plate_Crude/Plate_Crude</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<WorkToMake>7500</WorkToMake>
			<MaxHitPoints>180</MaxHitPoints>
			<ArmorRating_Blunt>0.50</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.15</ArmorRating_Sharp>
			<ArmorRating_Heat>0.05</ArmorRating_Heat>
			<Insulation_Cold>-13</Insulation_Cold>
			<Insulation_Heat>3</Insulation_Heat>
			<Mass>7</Mass>
			<Bulk>20</Bulk>
			<WornBulk>10</WornBulk>
			<EquipDelay>10</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<CarryBulk>10</CarryBulk>
			<MoveSpeed>-0.20</MoveSpeed>
			<WorkSpeedGlobal>-0.3</WorkSpeedGlobal>
			<MentalBreakThreshold>-0.06</MentalBreakThreshold>
			<ShootingAccuracyPawn>-0.12</ShootingAccuracyPawn>
			<AimingDelayFactor>0.15</AimingDelayFactor>
			<MeleeHitChance>-0.25</MeleeHitChance>
			<MeleeWeapon_CooldownMultiplier>0.26</MeleeWeapon_CooldownMultiplier>
		</equippedStatOffsets>
		<generateCommonality>1.1</generateCommonality>
		<apparel>
			<wornGraphicPath>Things/Apparel/Plate_Crude/Plate_Crude</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>PredatorsUnique</li>
				<li>PredatorsLight</li>
				<li>FeralOutfit</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Arms</li>
				<li>Shoulders</li>
			</bodyPartGroups>
		</apparel>
		<thingCategories>
			<li>HeavyFullArmorCat</li>
		</thingCategories>
	</ThingDef>

