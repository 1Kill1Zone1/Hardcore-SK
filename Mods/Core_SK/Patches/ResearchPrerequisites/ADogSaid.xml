<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
    <mods>
        <li>A Dog Said... Animal Prosthetics</li>
    </mods>
    <match Class ="PatchOperationSequence">
            <operations>
                <li Class ="PatchOperationReplace">
                    <xpath>Defs/RecipeDef[@Name="OldWoundsAnimal"]/researchPrerequisite</xpath>
                    <value>
                        <researchPrerequisite>
                            <li>Animal_13</li>
                        </researchPrerequisite>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="TableAnimalProsthetics"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>Animal_31</li>
                        </researchPrerequisites>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/RecipeDef[@Name="ProstheticAnimalCraftBase"]/researchPrerequisite</xpath>
                    <value>
                        <researchPrerequisite>
                            <li>Animal_31</li>
                        </researchPrerequisite>
                    </value>
                </li>
            </operations>
    </match>
    </Operation>

</Patch>